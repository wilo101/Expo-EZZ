# Environment Variables
# Do not commit actual secrets to this file!
# Copy this file to .env and fill in the values

# Base API URL
EXPO_PUBLIC_BASE_URL=https://your-api-domain.com/api/v2

# API Key for authentication
EXPO_PUBLIC_API_KEY=your_api_key_here

# Store Hash
EXPO_PUBLIC_STORE_HASH=your_store_hash

# Domain
EXPO_PUBLIC_DOMAIN=your-domain.com

# Locale (default is en)
EXPO_PUBLIC_LOCALE=en